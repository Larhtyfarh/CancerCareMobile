{
  "expo": {
    "name": "cancercare",
    "slug": "cancercare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Logo.png",
    "splash": {
      "image": "./src/assets/images/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D9D4F6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "CancerCare is a mobile app to help people with Cancer keep track of their health."
  }
}
